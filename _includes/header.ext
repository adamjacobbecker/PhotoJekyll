  <header>
    <a href="/" class="site-title">WALKER OLESEN</a>
  </header>
  
  <ul class="nav">
    {% for post in site.posts %}
      <li class="{% if post.url == page.url %}current{% endif %}
                 {% if post.layout == 'default' %}lighter{% endif %}">
        <a href="{{ post.url }}">{{ post.title }}</a>
      </li>
    {% endfor %}
  </ul>

